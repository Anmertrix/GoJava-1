<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="quoteDao" class="belskii.artem.kickstarter.dao.quote.QuoteDaoImplHiber"/>
	<bean id="categoryDaoImpl" class="belskii.artem.kickstarter.dao.category.CategoryDaoImplHiber"/>
	<bean id="projectDaoImpl" class="belskii.artem.kickstarter.dao.project.ProjectDaoImplHiber"/>
	
	<bean id="categoryModel" class="belskii.artem.kickstarter.mvc.model.CategoryModel">
   		<constructor-arg name="categoryDao" ref="categoryDaoImpl"/>
	</bean>
	

</beans>