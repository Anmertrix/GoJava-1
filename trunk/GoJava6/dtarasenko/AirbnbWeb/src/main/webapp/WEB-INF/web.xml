<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="true">

  <display-name>Archetype Created Web Application</display-name>


    <servlet>
        <servlet-name>UserRegistrationForm</servlet-name>
        <servlet-class>com.gojava6.airbnb.web.UserRegistrationForm</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserRegistrationForm</servlet-name>
        <url-pattern>/UserRegistrationForm</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowUsers</servlet-name>
        <servlet-class>com.gojava6.airbnb.web.ShowUsers</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShowUsers</servlet-name>
        <url-pattern>/ShowUsers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowApartments</servlet-name>
        <servlet-class>com.gojava6.airbnb.web.ShowApartments</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShowApartments</servlet-name>
        <url-pattern>/ShowApartments</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowReservations</servlet-name>
        <servlet-class>com.gojava6.airbnb.web.ShowReservations</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShowReservations</servlet-name>
        <url-pattern>/ShowReservations</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>com.gojava6.airbnb.web.Search</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>




    <servlet>
    <servlet-name>Test</servlet-name>
    <servlet-class>com.gojava6.airbnb.web.Test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>




    <resource-ref>
        <description>DB Connection2</description>
        <res-ref-name>jdbc/ResAirbnb</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>

</web-app>
