CREATE TABLE `airbnb`.`apartment` (
  `id` INT NOT NULL,
  `adress` VARCHAR(120) NOT NULL,
  `apartmenttype` TINYINT NOT NULL,
  `user` INT NOT NULL,
  `city` INT NOT NULL,
  INDEX `user` (`user` ASC),
  INDEX `city` (`city` ASC),
  PRIMARY KEY (`id`),
    FOREIGN KEY (`user`)
    REFERENCES `airbnb`.`user` (`id`),
    FOREIGN KEY (`city`)
    REFERENCES `airbnb`.`city` (`id`)
    );