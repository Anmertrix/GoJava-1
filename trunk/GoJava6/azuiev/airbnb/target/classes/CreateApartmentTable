CREATE  TABLE `airbnb`.`apartment` (
  `id` INT NOT NULL ,
  `adress` VARCHAR(120) NOT NULL ,
  `aparttype` TINYINT NOT NULL ,
  `user` INT NOT NULL ,
  `city` INT NOT NULL ,
  PRIMARY KEY (`id`) ,
  INDEX `user_apartment` (`user` ASC) ,
  INDEX `city_apartment` (`city` ASC) ,
  CONSTRAINT `user_apartment`
    FOREIGN KEY (`user` )
    REFERENCES `airbnb`.`user` (`id` )
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `city_apartment`
    FOREIGN KEY (`city` )
    REFERENCES `airbnb`.`city` (`id` )
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);
