<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ua.com.goit.gojava7.kickstarter.beans">

	<class name="Reward" table="reward">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="pledge" type="integer" />
		<property name="description" type="string" />
		<many-to-one name="project" column="project_id" class="ua.com.goit.gojava7.kickstarter.beans.Project" not-null="true" />
	</class>

</hibernate-mapping>